                            <msb>0</msb>
                            <dim>32</dim>
                            <dimIncrement>1</dimIncrement>
                        </field>
                    </fields>
                </register>
                <register>
                    <name>rise_ip</name>
                    <description>Rise Interrupt Pending Register</description>
                    <addressOffset>0x01C</addressOffset>
                    <fields>
                        <field>
                            <name>pin%s</name>
                            <lsb>0</lsb>
                            <msb>0</msb>
                            <dim>32</dim>
                            <dimIncrement>1</dimIncrement>
                        </field>
                    </fields>
                </register>
                <register>
