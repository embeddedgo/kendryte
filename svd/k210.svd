                        </field>
                    </fields>
                </register>
                <register>
                    <name>iof_en</name>
                    <description>HW I/O Function Enable Register</description>
                    <addressOffset>0x038</addressOffset>
                    <fields>
                        <field>
                            <name>pin%s</name>
                            <lsb>0</lsb>
                            <msb>0</msb>
                            <dim>32</dim>
