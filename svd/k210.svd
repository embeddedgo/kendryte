                    <addressOffset>0x010</addressOffset>
                    <fields>
                        <field>
                            <name>pin%s</name>
                            <lsb>0</lsb>
                            <msb>0</msb>
                            <dim>32</dim>
                            <dimIncrement>1</dimIncrement>
