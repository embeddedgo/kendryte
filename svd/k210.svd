                    </fields>
                </register>
                <register>
                    <name>iof_sel</name>
                    <description>HW I/O Function Select Register</description>
                    <addressOffset>0x03C</addressOffset>
                    <fields>
                        <field>
                            <name>pin%s</name>
                            <lsb>0</lsb>
                            <msb>0</msb>
                            <dim>32</dim>
                            <dimIncrement>1</dimIncrement>
                        </field>
                    </fields>
                </register>
                <register>
                    <name>output_xor</name>
